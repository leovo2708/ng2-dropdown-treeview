{
  "name": "ng2-dropdown-treeview",
  "version": "2.0.1",
  "description": "An Angular 2 tree component with checkbox and multiple level",
  "scripts": {
    "sass": "node-sass src -o src --output-style=expanded --indent-width=4",
    "prepublish": "npm run ngc && npm run build",
    "ngc": "ngc -p tsconfig.json",
    "build": "webpack --config config/webpack.lib.js",
    "lint": "tslint \"src/**/*.ts\" -e \"src/**/*.d.ts\"",
    "test": "karma start config/karma.conf.js",
    "demo:lint": "tslint \"demo/**/*.ts\"",
    "demo:dev": "webpack-dev-server --inline --progress --open --port 12346 --config demo/config/webpack.dev.js",
    "demo:prod": "webpack --config demo/config/webpack.prod.js --progress --profile --bail"
  },
  "keywords": [
    "ng",
    "angular",
    "ng2",
    "angular2",
    "ng2-dropdown-treeview",
    "tree",
    "treeview",
    "tree-view",
    "checkbox",
    "dropdown",
    "recursive",
    "angular tree component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leovo2708/ng2-dropdown-treeview.git"
  },
  "author": "Leo Vo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leovo2708/ng2-dropdown-treeview/issues"
  },
  "main": "bundles/ng2-dropdown-treeview.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "homepage": "https://github.com/leovo2708/ng2-dropdown-treeview",
